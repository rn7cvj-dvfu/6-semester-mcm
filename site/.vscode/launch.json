{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--target",
        "lib/main.dart",
        "d",
        "chrome",
        "--web-browser-flag",
        "--disable-web-security",
        "--web-port",
        "8080",
      ]
    },

  ],
  "inputs": [
    {
      "id": "environment",
      "type": "pickString",
      "description": "Select the environment to run",
      "options": ["dev", "prod"],
      "default": "dev"
    },
  ]
}
